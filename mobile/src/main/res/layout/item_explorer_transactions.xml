<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_up_next"
        android:layout_marginEnd="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp">

    <androidx.appcompat.widget.AppCompatTextView
            android:text="Age"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView18"
            android:textSize="13sp"
            style="@style/TextAppearance.Subtitle1.Medium"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            android:layout_marginTop="16dp"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="Transaction ID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView22"
            android:textSize="13sp"
            style="@style/TextAppearance.Subtitle1.Medium"
            app:layout_constraintTop_toBottomOf="@+id/textView18"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/tvTransactionId"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="Confirmations"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView24"
            android:textSize="13sp"
            style="@style/TextAppearance.Subtitle1.Medium"
            app:layout_constraintTop_toBottomOf="@+id/textView22"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            android:layout_marginTop="16dp"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="Inputs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView27"
            android:textSize="13sp"
            style="@style/TextAppearance.Subtitle1.Medium"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/textView24"
            android:layout_marginTop="16dp"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="Outputs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView28"
            android:textSize="13sp"
            style="@style/TextAppearance.Subtitle1.Medium"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/textView27"
            android:layout_marginTop="16dp"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="Output"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView30"
            android:textSize="13sp"
            style="@style/TextAppearance.Subtitle1.Medium"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView28"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="Fees"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView31"
            android:textSize="13sp"
            style="@style/TextAppearance.Subtitle1.Medium"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/textView30"
            android:layout_marginTop="16dp"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="Fees per byte"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView32"
            android:textSize="13sp"
            style="@style/TextAppearance.Subtitle1.Medium"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/textView31"
            android:layout_marginTop="16dp"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="Size (kb)"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView33"
            android:textSize="13sp"
            style="@style/TextAppearance.Subtitle1"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="10dp"
            app:layout_constraintTop_toBottomOf="@+id/textView32"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"/>

    <com.bitcoin.wallet.btc.ui.widget.ShortTimeView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvAgeT"
            style="@style/TextAppearance.Subtitle1"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="@+id/textView18"
            app:layout_constraintBottom_toBottomOf="@+id/textView18"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/tvTransactionId"
            style="@style/TextAppearance.Subtitle1"
            android:textColor="@color/colorAccent"
            android:background="?android:selectableItemBackground"
            android:textSize="12sp"
            android:gravity="center_vertical|end"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="@+id/textView22"
            app:layout_constraintBottom_toBottomOf="@+id/textView22"
            app:layout_constraintStart_toEndOf="@+id/textView22"
            app:layout_constraintHorizontal_bias="0.5"
            android:layout_marginStart="8dp"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvConfirmT"
            style="@style/TextAppearance.Subtitle1"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="@+id/textView24"
            app:layout_constraintBottom_toBottomOf="@+id/textView24"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvInputs"
            style="@style/TextAppearance.Subtitle1"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="@+id/textView27"
            app:layout_constraintBottom_toBottomOf="@+id/textView27"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvOutputs"
            style="@style/TextAppearance.Subtitle1"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="@+id/textView28"
            app:layout_constraintBottom_toBottomOf="@+id/textView28"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvOutput"
            style="@style/TextAppearance.Subtitle1"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="@+id/textView30"
            app:layout_constraintBottom_toBottomOf="@+id/textView30"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvFees"
            style="@style/TextAppearance.Subtitle1"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="@+id/textView31"
            app:layout_constraintBottom_toBottomOf="@+id/textView31"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvFeePer"
            style="@style/TextAppearance.Subtitle1"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="@+id/textView32"
            app:layout_constraintBottom_toBottomOf="@+id/textView32"/>

    <androidx.appcompat.widget.AppCompatTextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvSizeT"
            style="@style/TextAppearance.Subtitle1"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="@+id/textView33"
            app:layout_constraintBottom_toBottomOf="@+id/textView33"/>
</androidx.constraintlayout.widget.ConstraintLayout>